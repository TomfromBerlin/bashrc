# What Shell do we have?
MYSHELL=$SHELL
if MYSHELL=/bin/sh ; then MYSHELL="Bourne Shell";
else
    if MYSHELL=/bin/bash ; then MYSHELL="Bourne Again Shell";
    else 
        if MYSHELL=/bin/zsh ; then MYSHELL="Z-Shell";
        else 
	        if MYSHELL=/bin/ksh ; then MYSHELL="Korn-Shell";
            else
                if MYSHELL=/bin/dash ; then MYSHELL=*Debian-Almquist-Shell*;
                else
                    $MYSHELL="Unknown shell type";
	            fi
	        fi
        fi 
    fi
fi
