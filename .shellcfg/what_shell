# What Shell do we have?
MYSHELL=$SHELL
if MYSHELL=/bin/sh/ ; then MYSHELL=Bourne Shell (sh);
else
    if MYSHELL=/bin/bash ; then MYSHELL=Bourne Again Shell (bash);
    else 
        if MYSHELL=/bin/zsh ; then MYSHELL=Z-Shell (zsh);
        else 
	        if MYSHELL=/bin/ksh ; then MYSHELL=Korn-Shell (ksh);
            else
                if MYSHELL=/bin/dash ; then MYSHELL=Debian-Almquist-Shell (dash);
                else
                    $MYSHELL= Unknown shell type ;
	            fi
	        fi
        fi 
    fi
fi
